<h1>Hello, Welcome to auth app.</h1>
<li><%= link_to 'LinkedIn', '/auth/linkedin' %></li>
<li><%= link_to "Facebook", "/auth/facebook", id: "sign_in" %></li>
<% if user_signed_in?%>
  <h5>Available:</h5>
  <%= current_user.email%>|
  <%= link_to 'Sign Out',destroy_user_session_path,:method => :delete , :class => 'btn btn-xs btn-info'%>
<% else %>
  <%= link_to 'Sign In','users/sign_in', :class => 'btn btn-xs btn-info'%>
<% end %>